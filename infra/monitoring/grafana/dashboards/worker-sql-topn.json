{
  "uid": "worker-sql-topn",
  "title": "Worker SQL Top N",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "table",
      "title": "Top 10 Active Queries by Duration (pg_stat_activity)",
      "datasource": "PostgresTarget",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT pid, usename, state, EXTRACT(EPOCH FROM (now() - query_start)) * 1000 AS duration_ms, query FROM pg_stat_activity WHERE state <> 'idle' AND query IS NOT NULL AND query NOT ILIKE '%pg_stat_activity%' ORDER BY duration_ms DESC LIMIT 10;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "duration_ms" },
            "properties": [
              { "id": "unit", "value": "ms" }
            ]
          }
        ]
      },
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 12 }
    }
  ],
  "templating": { "list": [] },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
