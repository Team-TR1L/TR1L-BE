{
  "family": "td-worker",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "taskRoleArn": "arn:aws:iam::234588504179:role/role-worker",
  "executionRoleArn": "arn:aws:iam::234588504179:role/ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "worker",
      "image": "234588504179.dkr.ecr.ap-northeast-2.amazonaws.com/worker:scheduled",
      "cpu": 0,
      "portMappings": [
        {
          "name": "worker-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        { "name": "PG_SUB_DB", "value": "postgre" },
        { "name": "S3_BUCKET", "value": "billing-tr1l-apne2" },
        { "name": "PG_SUB_USER", "value": "subroot" },
        { "name": "PG_MAIN_PASSWORD", "value": "mainroot0000" },
        { "name": "PG_SUB_HOST", "value": "rds-tr1l-sub.clmq422cskwt.ap-northeast-2.rds.amazonaws.com" },
        { "name": "PG_SUB_PORT", "value": "5432" },
        { "name": "PG_MAIN_HOST", "value": "rds-tr1l-main.clmq422cskwt.ap-northeast-2.rds.amazonaws.com" },
        { "name": "PG_MAIN_PORT", "value": "5432" },
        { "name": "PG_SUB_PASSWORD", "value": "subroot0000" },
        { "name": "PG_MAIN_DB", "value": "postgre" },
        { "name": "PG_MAIN_USER", "value": "mainroot" }
      ],
      "secrets": [
        {
          "name": "MONGODB_URI",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:234588504179:parameter/tr1l/mongodb_uri"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/td-worker",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
